{
  "name": "bingobanko",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "engines": {
    "node": "^24.14.0"
  },
  "packageManager": "pnpm@10.30.3",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run type-check && vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json && tsc --noEmit -p tsconfig.node.json",
    "deploy:worker": "pnpm run build && wrangler deploy --config wrangler.jsonc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@hugeicons/core-free-icons": "^3.3.0",
    "@hugeicons/vue": "^1.0.4",
    "vue": "^3.5.29",
    "vue-i18n": "^11.2.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260305.0",
    "@eslint/js": "^10.0.1",
    "@tsconfig/node24": "^24.0.4",
    "@types/node": "^24.10.14",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^10.0.2",
    "eslint-plugin-vue": "^10.8.0",
    "globals": "^17.3.0",
    "happy-dom": "^20.7.0",
    "jiti": "^2.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-eslint-parser": "^10.4.0",
    "vue-tsc": "^3.2.5",
    "wrangler": "^4.68.1"
  },
  "pnpm": {
    "overrides": {
      "glob": "latest",
      "minimatch": "^10.2.1"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
